{
  "name": "Java",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",

  "features": {},

  "remoteEnv": {
    "GRADLE_USER_HOME": "/workspace/.gradle",
    "DB_HOST": "postgres",
    "DB_PORT": "5432",
    "DB_NAME": "appdb",
    "DB_USER": "appuser",
    "DB_PASSWORD": "apppass",
    "SPRING_PROFILES_ACTIVE": "dev"
  },

  "portsAttributes": {
    "8080": {"onAutoForward": "silent"},
    "5432": {"onAutoForward": "silent"},
    "35679": {"onAutoForward": "silent"},
    "35697": {"onAutoForward": "silent"}
  }
}
